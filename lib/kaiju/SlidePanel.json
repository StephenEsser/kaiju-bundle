{
  "name": "SlidePanel",
  "library": "terra-slide-panel",
  "display": "Slide Panel",
  "description": "Provides a progessive disclosure mechanism for cotent",
  "group": "Components",
  "properties": {
    "mainContent": {
      "type": "Component"
    },
    "panelContent": {
      "type": "Component"
    },
    "isOpen": {
      "type": "Bool",
      "display": "Open"
    },
    "isFullscreen": {
      "type": "Bool",
      "display": "Full screen"
    },
    "fill": {
      "type": "Bool"
    },
    "panelBehavior": {
      "type": "String",
      "form_type": "CodifiedList",
      "default": "overlay",
      "options": [
        {
          "display": "Overlay",
          "value": "overlay",
          "type": "String"
        },
        {
          "display": "Squish",
          "value": "squish",
          "type": "String"
        }
      ]
    },
    "panelPosition": {
      "type": "String",
      "form_type": "CodifiedList",
      "default": "end",
      "options": [
        {
          "display": "Start",
          "value": "start",
          "type": "String"
        },
        {
          "display": "End",
          "value": "end",
          "type": "String"
        }
      ]
    },
    "panelSize": {
      "type": "String",
      "form_type": "CodifiedList",
      "default": "small",
      "options": [
        {
          "display": "Small",
          "value": "small",
          "type": "String"
        },
        {
          "display": "Large",
          "value": "large",
          "type": "String"
        }
      ]
    }
  }
}
