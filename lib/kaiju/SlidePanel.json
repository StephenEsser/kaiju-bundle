{
  "name": "SlidePanel",
  "library": "terra-slide-panel",
  "display": "Slide Panel",
  "description": "Provides a progessive disclosure mechanism for cotent",
  "group": "SlidePanel",
  "properties": {
    "mainContent": {
      "type": "Component",
      "display": "Main content"
    },
    "panelContent": {
      "type": "Component",
      "display": "Panel content"
    },
    "isOpen": {
      "type": "Boolean",
      "display": "Open"
    },
    "isFullscreen": {
      "type": "Boolean",
      "display": "Full screen"
    },
    "fill": {
      "type": "Boolean",
      "display": "Fill container"
    },
    "panelBehavior": {
      "type": "CodifiedList",
      "default": "overlay",
      "display": "Panel behavior",
      "options": [
        {
          "display": "Overlay",
          "value": "overlay",
          "type": "String"
        },
        {
          "display": "Squish",
          "value": "squish",
          "type": "String"
        }
      ]
    },
    "panelPosition": {
      "type": "CodifiedList",
      "display": "Panel position",
      "default": "end",
      "options": [
        {
          "display": "Start",
          "value": "start",
          "type": "String"
        },
        {
          "display": "End",
          "value": "end",
          "type": "String"
        }
      ]
    },
    "panelSize": {
      "type": "CodifiedList",
      "default": "small",
      "display": "Panel size",
      "options": [
        {
          "display": "Small",
          "value": "small",
          "type": "String"
        },
        {
          "display": "Large",
          "value": "large",
          "type": "String"
        }
      ]
    }
  }
}
