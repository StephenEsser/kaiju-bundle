{
  "name": "DemographicsBanner",
  "library": "terra-demographics-banner",
  "display": "Demographics Banner",
  "description": "Displays demographics information",
  "group": "Content",
  "properties": {
    "personName": {
      "type": "String",
      "default": "John Smith",
      "display": "Person name"
    },
    "age": {
      "type": "String",
      "default": "25 Years",
      "display": "Age"
    },
    "dateOfBirthLabel": {
      "type": "String",
      "default": "DOB",
      "display": "DOB Label"
    },
    "dateOfBirth": {
      "type": "String",
      "default": "May 9, 1993",
      "display": "Date of birth"
    },
    "gender": {
      "type": "String",
      "default": "Male",
      "display": "Gender"
    },
    "photo": {
      "type": "Component",
      "display": "Photo",
      "default": {
        "type": "terra-image::Image",
        "props": {
          "src": "data:image/svg+xml;base64,",
          "alt": "patient-image",
          "height": "50px",
          "width": "50px"
        }
      }
    },
    "gestationalAgeLabel": {
      "type": "String",
      "display": "Gestational age label"
    },
    "gestationalAge": {
      "type": "String",
      "display": "Gestational age"
    },
    "postMenstrualAgeLabel": {
      "type": "String",
      "display": "Menstrual age label"
    },
    "postMenstrualAge": {
      "type": "String",
      "display": "Post menstrual age"
    },
    "deceasedDate": {
      "type": "String",
      "display": "Deceased date"
    },
    "applicationContent": {
      "type": "Array",
      "display": "Additional content",
      "schema": {
        "type": "Component"
      }
    }
  }
}
