{
  "name": "TextField",
  "library": "terra-form",
  "display": "Text Field",
  "description": "Arranges a standard TexField with an optional label, error, and value",
  "group": "Forms",
  "import_from": "terra-form/lib/TextField",
  "properties": {
    "value": {
      "type": "String"
    },
    "minLength": {
      "type": "Number"
    },
    "maxLength": {
      "type": "Number"
    },
    "label": {
      "type": "String",
      "default": "Label"
    },
    "error": {
      "type": "String"
    },
    "help": {
      "type": "String"
    },
    "name": {
      "type": "String"
    },
    "inputId": {
      "type": "String"
    },
    "required": {
      "type": "Bool"
    },
    "isInline": {
      "type": "Bool",
      "display": "Inline"
    }
  }
}
